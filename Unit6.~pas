{$R *.dfm}

procedure TForm6.Button1Click(Sender: TObject);
begin
Form4.Show;
Form6.Hide;
end;

procedure TForm6.N13Click(Sender: TObject);
begin
Form1.Show;
Form21.Show;
Form6.Hide;
end;

